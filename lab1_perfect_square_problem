#include <iostream>
using namespace std;

int main()
{
    int i = 1; //initializing value of i with 1  

    while(i <= 100){ //using a while loop to test all the values from 1 to 100
        int j = i*i;
        if((j % 10)% 2 == 1){ //seprating each digit in the number
            if(((j/10)% 10)% 2 == 1) //checking if the last two digits are odd
            cout << "The first perfect square number in which the last two digits are odd is: "<< j << endl;
        }
        i++; //increase the value of i to test all the numbers from 1 to 100
        if(i > 100)
            cout<<"The perfect square number in which the last two digits are odd does not exist."<<endl;
    }
    return 0;
}
